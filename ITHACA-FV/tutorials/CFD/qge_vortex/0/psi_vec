FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      psi_vec;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];   //spatial derivative of psi (dimensions = [0 1 -1 0 0 0 0]) 

internalField   uniform (0 0 0);

boundaryField
{
    fixedWalls
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }

    frontAndBack
    {
        type            empty;
    }
}

